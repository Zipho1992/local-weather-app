{
  "name": "local-weather",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "style:fix": "import-sort --write \" ***/{src, tests, e2e}/* .ts\" && prettier --write \"**/{src,tests,e2e}/*.{*css,ts}\" && jsbeautify\"src/**/*.html\"",
    "style": "import-sort -1 \"**/{src,tests,e2e}/*.ts\" && prettier --check \"**/{src,tests,e2e}/*.{*css,ts}\"",
    "lint": "tslint --config tslint.json --project .-e\"**/{test, polyfills}.ts\"",
    "lint:fix": "tslint --config tslint.json --fix --project . -e\"**/{test, polyfills}.ts\"",
    "start": "ng serve --port 5000",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --prod",
    "prenow:publish": "npm run build:prod",
    "now:publish": "now --platform-version 2 dist/local-weather-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.6",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.6",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "rxjs": "~7.5.0",
    "subsink": "^1.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "^13.2.6",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "angular-unit-test-helper": "^12.0.0",
    "codelyzer": "^6.0.2",
    "cross-conf-env": "^1.2.1",
    "dev-norms": "^1.7.1",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "init-dev-env": "^1.0.0",
    "jasmine": "^4.0.2",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "js-beautify": "^1.14.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "~2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "pa11y": "^6.1.1",
    "pa11y-ci": "^3.0.1",
    "prettier": "^2.5.1",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.10",
    "tslint-plugin-prettier": "^2.3.0",
    "tsutils": "^3.21.0",
    "typescript": "~4.5.2",
    "vercel": "^24.0.0"
